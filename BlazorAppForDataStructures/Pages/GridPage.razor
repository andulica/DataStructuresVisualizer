@page "/grid-page"

<table>
    @for (int i = 0; i < grid.GetLength(0); i++)
    {
            <tr>
            @for (int j = 0; j < grid.GetLength(1); j++)
            {
                        <td>
                    @if (grid[i, j].HasValue)
                    {
                                    <Node Data="@grid[i, j].Value" />
                    }
                    else
                    {
                        @:Blank
                    }
                        </td>
            }
            </tr>
    }
</table>

@code {
    private int?[,] grid;

    protected override void OnInitialized()
    {
        int rows = 5; 
        int columns = 5; 
        grid = new int?[rows, columns];

        // Initialize some cells with node data for demonstration
        grid[0, 0] = 1; // Node with data '1'
        grid[1, 1] = 2; // Node with data '2'
        // Other cells will be null, representing blank cells
    }
}


